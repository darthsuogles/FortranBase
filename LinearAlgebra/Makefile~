all: linAlg_test

linAlg_test: linAlg_test.f90 mLinAlg.o mGutenberg.o mConstants.o
	ifort linAlg_test.f90 -o linAlg_test mLinAlg.o mGutenberg.o 


mLinAlg.o: mLinAlg.f90 mConstants.f90
	ifort -c mConstants.f90
	ifort -fpp -c -DMemory mLinAlg.f90


mGutenberg.o: mGutenberg.f90 mLinAlg.o
	ifort -c mGutenberg.f90

clean:
	rm *.o *.mod