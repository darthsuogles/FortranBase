program LinAlg_test

use mLinAlg
use mGutenberg

implicit none

type(realMatrixType):: rmat
type(complexMatrixType):: cmat
integer:: m = 4, n = 3

call AllocMatrix(rmat, m, n, 0)
call AllocMatrix(cmat, m, n, 0)

call WriteMatrix(rmat)
call WriteMatrix(cmat)

call DestroyMatrix(rmat, 0)
call DestroyMatrix(cmat, 0)

end program LinAlg_test
